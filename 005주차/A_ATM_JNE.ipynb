   "source": [
    "# 인원 수\n",
    "n = int(input())\n",
    "# 인출 시간\n",
    "p = list(map(int, input().split()))\n",
    "\n",
    "# 인출 시간 오름차순\n",
    "p.sort()\n",
    "\n",
    "# n번째 사람이 걸린 시간\n",
    "waiting = 0\n",
    "# 전체 누적 시간\n",
    "total_waiting = 0\n",
    "for i in p:\n",
    "    waiting += i\n",
    "    total_waiting += waiting\n",
    "\n",
    "total_waiting"
   ]
  }
